.code-bar
  .label.label-info KEYS
  span.code {{frame.input | uncomment}}
  .actions
    a.icon-trash(ng-click='destroyFrame(frame)', tooltip-placement='bottom', tooltip='Remove')

.inner
  .view-result.view-keys
    h4 Keyboard shortcuts
    ul
      li
        .left-col
          .key.code Ctrl-Enter
        .right-col
          | Execute current query
      li
        .left-col
          .key.code Ctrl-Up
        .right-col
          | Next command in history
      li
        .left-col
          .key.code Ctrl-Down
        .right-col
          | Previous command in history



