.code-bar
  .label.label-info Help
  span.code {{frame.input | uncomment}}
  .actions
    a.btn.btn-small
      i.icon-collapse-alt
    a.btn.btn-small(ng-click='destroyFrame(frame)')
      i.icon-remove

.inner
  .view-result.boxed(ng-hide='frame.hasErrors')
    div(ng-include = 'frame.response.page')
